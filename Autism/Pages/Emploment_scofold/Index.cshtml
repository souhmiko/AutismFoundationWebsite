            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployLearning.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployTrain.Name)
            </td>
